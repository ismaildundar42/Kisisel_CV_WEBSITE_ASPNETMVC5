
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">

    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı Giriniz..." />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlığı Giriniz..." />
    <br />
    <textarea name="Aciklama" class="form-control" rows="5" placeholder="Açıklamayı Giriniz..."></textarea>
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarihi Giriniz..." />
    <br />
    <button class="btn btn-success">Kaydet</button>

</form>

